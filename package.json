{
	"name": "fluent-svelte",
	"version": "1.0.91",
	"description": "A faithful implementation of Microsoft's Fluent Design System in Svelte.",
	"homepage": "https://github.com/tropix126/fluent-svelte",
	"author": {
		"name": "tropix126",
		"url": "https://github.com/tropix126/"
	},
	"keywords": [
		"design",
		"design-system",
		"design-language",
		"microsoft",
		"fluent",
		"fluentui",
		"fluent-design",
		"fluent-design-system",
		"svelte",
		"svelte3",
		"sveltejs",
		"svelte-component",
		"svelte-components",
		"ui-components",
		"winui",
		"windows"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tropix126/fluent-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/tropix126/fluent-svelte/issues"
	},
	"scripts": {
		"dev": "tsm ./generateDocsExamples.ts && svelte-kit dev",
		"build": "tsm ./generateDocsExamples.ts && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"generate": "tsm ./generateDocsExamples.ts"
	},
	"devDependencies": {
		"@fluentui/svg-icons": "^1.1.150",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "^1.0.0-next.196",
		"@sveltejs/svelte-repl": "^0.2.2",
		"@types/prismjs": "^1.16.6",
		"autoprefixer": "^10.4.0",
		"cssnano": "^5.0.10",
		"focus-trap": "^6.7.1",
		"mdsvex": "^0.9.8",
		"mousetrap": "^1.6.5",
		"postcss": "^8.3.11",
		"postcss-variables-prefixer": "^1.1.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prism-svelte": "^0.4.7",
		"prismjs": "^1.25.0",
		"sass": "^1.43.4",
		"svelte": "^3.44.1",
		"svelte-check": "^2.2.9",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.9",
		"tslib": "^2.3.1",
		"tsm": "^2.1.4",
		"typescript": "^4.4.4"
	},
	"type": "module"
}
